{
  "name": "cards-against-way-good",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch './server' -e ts --exec 'node --loader @esbuild-kit/esm-loader ./server/index.dev.ts'",
    "build": "tsc -b && vite build",
    "docker": "docker-compose up --build",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:publish": "docker build -t adalfonso/cayf:latest . && docker push adalfonso/cayf:latest",
    "check": "tsc --noEmit && pnpm lint",
    "lint": "eslint --quiet"
  },
  "dependencies": {
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "@types/express": "^4.17.21",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "lodash-es": "^4.17.21",
    "preact": "^10.22.0",
    "sass": "^1.77.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@esbuild-kit/esm-loader": "^2.6.5",
    "@preact/preset-vite": "^2.8.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint": "^9.6.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.2",
    "typescript": "^5.2.2",
    "vite": "^5.3.1"
  }
}
